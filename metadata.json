{
  "name": "cirrax-libvirt",
  "version": "5.1.0",
  "author": "Cirrax GmbH",
  "summary": "Puppet module to install libvirt and create virtual domain configuration",
  "license": "GPL-3.0",
  "source": "https://github.com/cirrax/puppet-libvirt",
  "project_page": "https://github.com/cirrax/puppet-libvirt",
  "issues_url": "https://github.com/cirrax/puppet-libvirt/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=3.0.0 < 10.0.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 1.0.0 < 10.0.0"
    },
    {
      "name": "puppetlabs/inifile",
      "version_requirement": ">= 2.0.0 < 7.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "11",
        "12",
        "13"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "20.04",
        "22.04",
        "24.04"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "AlmaLinux",
      "operatingsystemrelease": [
        "9"
      ]
    }
  ],
  "requirements": [
    {
      "name": "openvox",
      "version_requirement": ">=5.0.0 < 9.0.0"
    }
  ],
  "tags": [
    "libvirt",
    "kvm",
    "drbd",
    "virtualization"
  ]
}
